{
  "name": "medicationsearch",
  "description": "node and react medication searcher",
  "version": "0.1.0",
  "author": "ajayp",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "fuse.js": "^3.6.1",
    "helmet": "^3.23.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "throttle-debounce": "^2.2.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "jest": "24.9.0",
    "supertest": "^4.0.2"
  },
  "proxy": "http://0.0.0.0:4000",
  "scripts": {
    "build": "node ./buildScript",
    "start-front": "react-scripts start",
    "build-front": "react-scripts build",
    "test-front": "react-scripts test",
    "eject-front": "react-scripts eject",
    "start-server": "node server/server.js",
    "start-server:debug": "cross-env NODE_ENV=development nodemon --inspect server/server.js ",
    "start": "npm run start-server",
    "test-server": "cross-env NODE_ENV=development jest ./server/**/*.test.js",
    "test-server:debug": "cross-env NODE_ENV=development node --inspect node_modules/.bin/jest --testTimeout=10000 ./server/**/*.test.js",
    "heroku-postbuild": "npm install && npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "./node_modules/"
    ],
    "verbose": true
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}